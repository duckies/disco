{
  "name": "@duckies/discord",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "start:build": "node dist/index.js",
    "sync": "bun src/scripts/sync.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "discord.js": "14.15.2",
    "glob": "10.4.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "9.0.0",
    "@types/bun": "latest",
    "@types/eslint__js": "8.42.3",
    "eslint": "9.0.0",
    "eslint-plugin-import-x": "0.5.0",
    "eslint-plugin-unicorn": "53.0.0",
    "typescript": "5.4.5",
    "typescript-eslint": "8.0.0-alpha.18",
    "unbuild": "2.0.0"
  }
}
